  <transitions name="Yellow-to-Red">
    <timer time="5"/>
  </transitions>
  <transitions name="Red-toYellow">
    <timer time="15"/>
  </transitions>
  <transitions name="Yellow-to-Green">
    <timer time="5"/>
  </transitions>
  <states name="Red"/>
  <states name="Yellow"/>
  <states name="Green"/>
</trafficlight:StateMachine>
