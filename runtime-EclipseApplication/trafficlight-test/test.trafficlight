<?xml version="1.0" encoding="UTF-8"?>
<trafficlight:StateMachine xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:trafficlight="http://www.mydsl.no/trafficlight/1.0" name="TrafficLightStateMachine">
  <transitions name="green-to-yellow">
    <timer time="15"/>
  </transitions>
  <transitions name="yellow-to-red">
    <timer time="5"/>
  </transitions>
  <transitions name="red-to-green">
    <timer time="20"/>
  </transitions>
  <states name="red"/>
  <states name="yellow"/>
  <states name="green"/>
</trafficlight:StateMachine>
